<div class="right_col" role="main">
    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_title">
					<h2> Shop Creation </h2>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">

                	<%= form_for(:shop, :url => {:action => 'create', :id => @shop.id}) do |f| %>

						<% @action = "new" %>

					    <%= render(:partial => "form", :locals => {:f => f}) %>
					    <div class="row">
					    	<div class="col-sm-8 col-sm-offset-2">
							    
							    <%= submit_tag("Submit", :class => "btn-primary btn") %>
							    <%= link_to("Cancel",{:controller => 'shops', :action => 'index'}, :class => "btn-danger btn") %>
							</div>
						</div>
						<br/><br/>
					<% end %>
                	
                </div>
            </div>
        </div>
    </div>
</div>
